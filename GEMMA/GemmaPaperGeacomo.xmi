      <expresionIzquierda xsi:type="Gemma:Variable" name="ManualCheck" tipo="privada"/>
      <expresionDerecha xsi:type="Gemma:Variable" name="RestingPosition" tipo="privada"/>
    </expresionBinaria>
  </transiciones>
  <transiciones xsi:type="Gemma:TrasicionEntreOmOm" name="A5_A6_T" origen="//@macroOms.0/@oms.2" destino="//@macroOms.0/@oms.0"/>
</Gemma:Gemma>

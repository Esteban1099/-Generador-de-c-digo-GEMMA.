<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated gemmaCreator -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="Gemma.presentation.GemmaModelWizardID"
            name="%_UI_GemmaModelWizard_label"
            class="Gemma.presentation.GemmaModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/GemmaModelFile.gif">
         <description>%_UI_GemmaModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated gemmaCreator -->
      <editor
            id="Gemma.presentation.GemmaEditorID"
            name="%_UI_GemmaEditor_label"
            icon="icons/full/obj16/GemmaModelFile.gif"
            extensions="gemma"
            class="Gemma.presentation.GemmaEditor"
            contributorClass="Gemma.presentation.GemmaActionBarContributor">
      </editor>
   </extension>

</plugin>
